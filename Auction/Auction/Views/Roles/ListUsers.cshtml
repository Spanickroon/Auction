@model IEnumerable<Auction.Data.Models.User>

<h2>Users list</h2>
<table class="table">
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Email</td>
        <td>
            <a class="btn btn-sm btn-primary" asp-area="" asp-controller="Roles" asp-action="EditRole" asp-route-userid="@user.Id">Edit role</a>
        </td>
        <td>
            <a class="btn btn-sm btn-primary" asp-area="" asp-controller="Account" asp-action="Edit" asp-route-id="@user.Id">Edit</a>
        </td>
    </tr>
    }
</table>